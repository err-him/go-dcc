    if test -z "`$DCC_XARGS -r echo bad </dev/null 2>&1`"; then
	DCC_XARGS="$DCC_XARGS -r"
    else
	if test -z "`$DCC_XARGS -n1000 echo bad </dev/null 2>&1`"; then
	    DCC_XARGS="$DCC_XARGS -n1000"
	fi
    fi
fi

